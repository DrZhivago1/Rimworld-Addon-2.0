<?xml version="1.0" encoding="UTF-8"?>
<RecipeDefs>


<RecipeDef ParentName="MakeWoodLumberBase">
		<defName>MakeWoodLumberMixedBundle</defName>
		<label>Bundle lumber together</label>
		<description>Bundle together random or spare pieces of lumber.</description>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>WoodLumber</li>
					</categories>
				</filter>
				<count>15</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>WoodLumber</li>
			</categories>
			<exceptedThingDefs>
				<li>WoodLog</li>
			</exceptedThingDefs>
		</fixedIngredientFilter>
		<products>
			<WoodLog>15</WoodLog>
		</products>
		<recipeUsers>
			<li>TableWoodworking</li>
		</recipeUsers>
	</RecipeDef>	

</RecipeDefs>